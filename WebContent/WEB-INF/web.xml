<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.lms.servlet.LogInServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>unregisterPlan</servlet-name>
		<servlet-class>com.lms.servlet.PlanUnregisterServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>unregisterPlan</servlet-name>
		<url-pattern>/unregisterPlan</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddUser</servlet-name>
		<servlet-class>com.lms.servlet.AddUserServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>AddUser</servlet-name>
		<url-pattern>/RegUser</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>mPayment</servlet-name>
		<servlet-class>com.lms.servlet.MonthlyPaymentServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>mPayment</servlet-name>
		<url-pattern>/mPayment</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>saveUserPlan</servlet-name>
		<servlet-class>com.lms.servlet.SaveUserPlanServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>saveUserPlan</servlet-name>
		<url-pattern>/saveUserPlan</url-pattern>
	</servlet-mapping>
	
	


	
</web-app>
	